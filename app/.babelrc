{
  "presets": [
    [
      "env",
      {
        "targets": {
          "node": 6
        },
        "useBuiltIns": true
      }
    ],
    "es2015",
    "stage-0",
    "react"
  ],
  "plugins": [
    "add-module-exports"
  ],
  "env": {
    "production": {
      "presets": [
        "react-optimize"
      ],
      "plugins": [
        "babel-plugin-dev-expression"
      ]
    },
    "development": {
      "sourceMaps": "both"
    },
    "test": {
    }
  }
}
